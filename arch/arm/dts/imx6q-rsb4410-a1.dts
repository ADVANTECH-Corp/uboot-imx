/dts-v1/;

#include "imx6q.dtsi"
#include "imx6qdl-advantech.dtsi"

/ {
	model = "i.MX6 Quad ADVANTECH Smart Device Board";
	compatible = "fsl,imx6q-sabresd", "fsl,imx6q";
        aliases {
                mmc0 = &usdhc2;
                mmc1 = &usdhc4;
                mmc2;
                mmc3;
        };

};

&uart1 {
	status = "okay";
};

&ecspi1 {
	status = "okay";
};

&fec {
	status = "okay";
};

&usdhc2 {
	status = "okay";
};

&usdhc4 {
	status = "okay";
};
